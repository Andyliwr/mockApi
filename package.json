{
  "name": "web-mock-server",
  "description": "A web system for mock-server,一个WEB版本的mock-server系统",
  "version": "2.0.0",
  "scripts": {
    "start": "node www/development.js",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir app/ --source-maps",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch": "npm run watch-compile"
  },
  "dependencies": {
    "babel-runtime": "6.x.x",
    "mockjs": "^1.0.1-beta3",
    "nunjucks": "^2.5.2",
    "request": "^2.75.0",
    "source-map-support": "0.4.0",
    "thinkjs": "2.2.x"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-stage-1": "6.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flftfqwxf/mock-server.git"
  },
  "license": "MIT",
  "main": "www/development.js",
  "keywords": [
    "mock-server",
    "nodejs",
    "thinkjs",
    "web"
  ],
  "author": "flftfqwxf",
  "bugs": {
    "url": "https://github.com/flftfqwxf/mock-server/issues"
  },
  "homepage": "https://github.com/flftfqwxf/mock-server#readme"
